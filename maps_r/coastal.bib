@article{holstiusFieldCalibrationsLowcost2014,
  title = {Field Calibrations of a Low-Cost Aerosol Sensor at a Regulatory Monitoring Site in {{California}}},
  author = {Holstius, D. M. and Pillarisetti, A. and Smith, K. R. and Seto, E.},
  date = {2014-04-30},
  journaltitle = {Atmospheric Measurement Techniques},
  volume = {7},
  number = {4},
  pages = {1121--1131},
  issn = {1867-1381},
  doi = {10.5194/amt-7-1121-2014},
  url = {https://www.atmos-meas-tech.net/7/1121/2014/},
  urldate = {2019-03-12},
  abstract = {{$<$}p{$><$}strong{$>$}Abstract.{$<$}/strong{$>$} Health effects attributed to ambient fine particulate matter (PM\textsubscript{2.5}) now rank it among the risk factors with the highest health burdens in the world, but existing monitoring infrastructure cannot adequately characterize spatial and temporal variability in urban PM\textsubscript{2.5} concentrations, nor in human population exposures. The development and evaluation of more portable and affordable monitoring instruments based on low-cost sensors may offer a means to supplement and extend existing infrastructure, increasing the density and coverage of empirical measurements and thereby improving exposure science and control. Here, we report on field calibrations of a custom-built, battery-operated aerosol monitoring instrument we developed using low-cost, off-the-shelf optical aerosol sensors. We calibrated our instruments using 1 h and 24 h PM\textsubscript{2.5} data from a class III US EPA Federal Equivalent Method (FEM) PM\textsubscript{2.5} β-attenuation monitor in continuous operation at a regulatory monitoring site in Oakland, California. We observed negligible associations with ambient humidity and temperature; linear corrections were sufficient to explain 60\% of the variance in 1 h reference PM\textsubscript{2.5} data and 72\% of the variance in 24 h data. Performance at 1 h integration times was comparable to commercially available optical instruments costing considerably more. These findings warrant further exploration of the circumstances under which this class of aerosol sensors may profitably be deployed to generate improved PM\textsubscript{2.5} data sets.{$<$}/p{$>$}},
  langid = {english},
  file = {/home/ash/Zotero/storage/VGDKHEPM/Holstius et al. - 2014 - Field calibrations of a low-cost aerosol sensor at.pdf;/home/ash/Zotero/storage/44JKQXMN/2014.html}
}

