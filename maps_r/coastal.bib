@article{lemeRoleSucroseCariogenic2006,
  title = {The {{Role}} of {{Sucrose}} in {{Cariogenic Dental Biofilm Formation}}—{{New Insight}}},
  author = {Leme, A.F. Paes and Koo, H. and Bellato, C.M. and Bedi, G. and Cury, J.A.},
  date = {2006-10-01},
  journaltitle = {Journal of Dental Research},
  shortjournal = {J Dent Res},
  volume = {85},
