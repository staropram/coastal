@article{crilleyEvaluationLowcostOptical2018,
  title = {Evaluation of a Low-Cost Optical Particle Counter ({{Alphasense OPC-N2}}) for Ambient Air Monitoring},
  author = {Crilley, Leigh R. and Shaw, Marvin and Pound, Ryan and Kramer, Louisa J. and Price, Robin and Young, Stuart and Lewis, Alastair C. and Pope, Francis D.},
  date = {2018-02-07},
  journaltitle = {Atmospheric Measurement Techniques},
  pages = {709--720},
  issn = {1867-8548},
  url = {https://doi.org/10.5194/amt-11-709-2018},
  urldate = {2019-03-12},
  abstract = {A fast-growing area of research is the development of low-cost sensors for measuring air pollutants. The affordability and size of low-cost particle sensors makes them an attractive option for use in experiments requiring a number of instruments such as high-density spatial mapping. However, for these low-cost sensors to be useful for these types of studies their accuracy and precision need to be quantified. We evaluated the Alphasense OPC-N2, a promising low-cost miniature optical particle counter, for monitoring ambient airborne particles at typical urban background sites in the UK. The precision of the OPC-N2 was assessed by co-locating 14 instruments at a site to investigate the variation in measured concentrations. Comparison to two different reference optical particle counters as well as a TEOM-FDMS enabled the accuracy of the OPC-N2 to be evaluated. Comparison of the OPC-N2 to the reference optical instruments shows some limitations for measuring mass concentrations of PM1, PM2.5 and PM10. The OPC-N2 demonstrated a significant positive artefact in measured particle mass during times of high ambient RH ({$>$}85\%) and a calibration factor was developed based upon °-Köhler theory, using average bulk particle aerosol hygroscopicity. Application of this RH correction factor resulted in the OPC-N2 measurements being within 33\% of the TEOM-FDMS, comparable to the agreement between a reference optical particle counter and the TEOM-FDMS (20\%). Inter-unit precision for the 14 OPC-N2 sensors of 22±13\% for PM10 mass concentrations was observed. Overall, the OPC-N2 was found to accurately measure ambient airborne particle mass concentration provided they are (i) correctly calibrated and (ii) corrected for ambient RH. The level of precision demonstrated between multiple OPC-N2s suggests that they would be suitable devices for applications where the spatial variability in particle concentration was to be determined.},
  langid = {english},
  file = {/home/ash/Zotero/storage/WDTKKH38/Crilley et al. - 2018 - Evaluation of a low-cost optical particle counter .pdf;/home/ash/Zotero/storage/CLXNTXYX/130354.html}
}

@article{crossUseElectrochemicalSensors2017,
  title = {Use of Electrochemical Sensors for Measurement of Air Pollution: Correcting Interference Response and Validating Measurements},
  shorttitle = {Use of Electrochemical Sensors for Measurement of Air Pollution},
  author = {Cross, Eben S. and Williams, Leah R. and Lewis, David K. and Magoon, Gregory R. and Onasch, Timothy B. and Kaminsky, Michael L. and Worsnop, Douglas R. and Jayne, John T.},
  date = {2017-09-29},
  journaltitle = {Atmospheric Measurement Techniques},
  volume = {10},
  number = {9},
  pages = {3575--3588},
  issn = {1867-1381},
  doi = {10.5194/amt-10-3575-2017},
  url = {https://www.atmos-meas-tech.net/10/3575/2017/},
  urldate = {2019-03-12},
  abstract = {{$<$}p{$><$}strong{$>$}Abstract.{$<$}/strong{$>$} The environments in which we live, work, and play are subject to enormous variability in air pollutant concentrations. To adequately characterize air quality (AQ), measurements must be fast (real time), scalable, and reliable (with known accuracy, precision, and stability over time). Lower-cost air-quality-sensor technologies offer new opportunities for fast and distributed measurements, but a persistent characterization gap remains when it comes to evaluating sensor performance under realistic environmental sampling conditions. This limits our ability to inform the public about pollution sources and inspire policy makers to address environmental justice issues related to air quality. In this paper, initial results obtained with a recently developed lower-cost air-quality-sensor system are reported. In this project, data were acquired with the ARISense integrated sensor package over a 4.5-month time interval during which the sensor system was co-located with a state-operated (Massachusetts, USA) air quality monitoring station equipped with reference instrumentation measuring the same pollutant species. This paper focuses on validating electrochemical (EC) sensor measurements of CO, NO, NO\textsubscript{2}, and O\textsubscript{3} at an urban neighborhood site with pollutant concentration ranges (parts per billion by volume, ppb; 5min averages, ±1\emph{σ}): [CO] = 231±116ppb (spanning 84–1706ppb), [NO] = 6.1±11.5ppb (spanning 0–209ppb), [NO\textsubscript{2}] = 11.7±8.3ppb (spanning 0–71ppb), and [O\textsubscript{3}] = 23.2±12.5ppb (spanning 0–99ppb). Through the use of high-dimensional model representation (HDMR), we show that interference effects derived from the variable ambient gas concentration mix and changing environmental conditions over three seasons (sensor flow-cell temperature = 23.4±8.5°C, spanning 4.1 to 45.2°C; and relative humidity = 50.1±15.3\%, spanning 9.8–79.9\%) can be effectively modeled for the Alphasense CO-B4, NO-B4, NO2-B43F, and Ox-B421 sensors, yielding (5min average) root mean square errors (RMSE) of 39.2, 4.52, 4.56, and 9.71ppb, respectively. Our results substantiate the potential for distributed air pollution measurements that could be enabled with these sensors.{$<$}/p{$>$}},
  langid = {english},
  file = {/home/ash/Zotero/storage/NZPZTPCY/Cross et al. - 2017 - Use of electrochemical sensors for measurement of .pdf;/home/ash/Zotero/storage/DXRT8I9B/2017.html}
}

