@article{singhSynergisticEffectsHeavy2017,
  title = {Synergistic {{Effects}} of {{Heavy Metals}} and {{Pesticides}} in {{Living Systems}}},
  author = {Singh, Nitika and Gupta, Vivek Kumar and Kumar, Abhishek and Sharma, Bechan},
